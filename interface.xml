<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN" "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
  <interface name="simonbru.SessionLaunch">
    <method name="Open">
      <arg name="workdir" type="s" direction="in"/>
      <arg name="executable" type="s" direction="in"/>
      <arg name="args" type="as" direction="in"/>
    </method>
    <method name="Exec">
      <arg name="workdir" type="s" direction="in"/>
      <arg name="executable" type="s" direction="in"/>
      <arg name="args" type="as" direction="in"/>
      <arg name="stdin" type="h" direction="in"/>
      <arg name="stdout" type="h" direction="in"/>
      <arg name="stderr" type="h" direction="in"/>
      <arg name="status" type="i" direction="out"/>
    </method>
  </interface>
</node>
